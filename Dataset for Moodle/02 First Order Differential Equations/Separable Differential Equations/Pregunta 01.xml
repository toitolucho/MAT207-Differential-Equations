  <question type="multichoice">
    <name>
      <text>Pregunta ParteA01</text>
    </name>
    <questiontext format="html">
      <text>&lt;p&gt;&lt;span style=&quot;color:red; font-weight: bold;&quot;&gt;Pregunta ParteA01&lt;/span&gt;&lt;/p&gt;&lt;p&gt;Resuelve la sig. E.D.:&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;\( \frac{dy}{dx} = 3x^2(1 + y^2)^{3/2} \)&lt;/p&gt;&lt;p&gt;&lt;strong&gt;a) Llevar la ecuación diferencial a su forma estándar.&lt;/strong&gt;&lt;/p&gt;</text>
    </questiontext>
    <answer fraction="100" format="html">
      <text>&lt;p&gt;\( \frac{1}{(1 + y^2)^{3/2}} \ dy = 3x^2 \ dx \)&lt;/p&gt;</text>
    </answer>
    <answer fraction="0" format="html">
      <text>\( \frac{dy}{(1 + y^2)^{2/3}} = 3x^2 \ dx \)</text>
      <feedback format="html">
        <text>Penalización: 50%</text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\( 3x^2 \ dx = \frac{1}{(1 + z^2)^{3/2}} \ dy \)</text>
      <feedback format="html">
        <text>Penalización: 50%</text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text>\( 3x^2 \ dx - (1 + y^2)^{3/2} \ dy = 0 \)</text>
      <feedback format="html">
        <text>Penalización: 50%</text>
      </feedback>
    </answer>
  </question>
